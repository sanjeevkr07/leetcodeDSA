class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {
        
        vector tempMat(matrix);
        int n=matrix.size();
        for(int i=0;i<n;i++){
            int temp=n-1;
            for(int j=0;j<n;j++){
                matrix[i][j]=tempMat[temp--][i];
            }
        }
        
    }
};
